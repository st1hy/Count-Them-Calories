<?xml version="1.0" encoding="utf-8"?>
<resources>
    
    <declare-styleable name="GraphColumn">
        <attr name="gc_orientation" format="enum">
            <enum name="right_to_left" value="1"/>
            <enum name="left_to_right" value="2"/>
            <enum name="top_to_bottom" value="3"/>
            <enum name="bottom_to_top" value="4"/>
        </attr>
        <attr name="gc_color" format="color"/>
        <attr name="gc_progress" format="float"/>
        <attr name="gc_mode">
            <flag name="none" value="0"/>
            <flag name="column" value="1"/>
            <flag name="line" value="2"/>
            <flag name="point" value="4"/>
            <flag name="all" value="7"/>
        </attr>
        <attr name="gc_line_width" format="dimension"/>
        <attr name="gc_line_color" format="color"/>
        <attr name="gc_legend_line_width" format="dimension"/>
        <attr name="gc_legend_line_color" format="color"/>
        <attr name="gc_legend_separator_size" format="dimension"/>
        <attr name="gc_legend_min_legend_line_space" format="dimension"/>
        <attr name="gc_legend_line_overlay_color" format="color"/>
        <attr name="gc_point_size" format="dimension"/>
        <attr name="gc_point_color" format="color"/>
        <attr name="gc_legend_text_size" format="dimension"/>
        <attr name="gc_legend_text_value_color" format="color"/>
        <attr name="gc_legend_text_point_color" format="color"/>
    </declare-styleable>
</resources>